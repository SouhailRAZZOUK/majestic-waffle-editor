{
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types",
      "./"
    ],
    "types": [
      "winjs",
      "declarations"
    ],
    "target": "ES6",
    "rootDir": "./",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": false,
    "outDir": "dist/"
  }
}